<!DOCTYPE html>
<html>
<head>
  <title>Booklibrary</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render "layouts/partials/navigation" %>
	<header>
		<%= render "layouts/partials/header" %>
	</header>
	<div id="container">
		<%= yield %>
	</div>
	<footer>
		<%= render "layouts/partials/footer" %>
	</footer>
</body>
</html>
